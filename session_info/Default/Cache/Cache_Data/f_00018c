        formReady("form", function(form) {
            var $form = $(form);
            
            //------------------------------------------------------------
            // 조회 버튼
            //------------------------------------------------------------
            $form.find("#btnSearch").click(function() {
            	enterSearch();
            });
            
            $('#reqBtnClose2').click(function(){
        		$('#DLayer').hide();
        		$('#modalPop').hide();
        	});
        	
        	$('#reqBtnClose1').click(function(){
        		$('#DLayer').hide();
        		$('#modalPop').hide();
        	});
        	
        	$("#trIssuReason").hide();
			
			var isClose = $.cookie('notice.popup.check');
        	
        	if(!isClose || isClose !== "close") {
    			//ajaxSubmit(popNoticeUri, param, popupNoticeAjaxCallback, null, null);
    			
